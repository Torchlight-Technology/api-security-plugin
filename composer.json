{
    "name": "torchlighttechnology/api-gateway",
    "description": "ApiGateway plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "aws/aws-sdk-php": "^3.55"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "ApiGateway\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApiGateway\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
